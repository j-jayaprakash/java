package com.jp.ums.dao;

import java.util.Iterator;

public class Test {

	public static void main(String[] args) {

		int[] a = { 1, 2, 3, 4 };

		int k = 0;
		int sum = 1;

		for (int i = 0; i < a.length; i++) {

			sum = 1;

			for (int j = 0; j < a.length; j++) {

				if (j != k) {
					sum *= a[i];
				}
				k++;

			}

			System.out.println(sum);
		}
	}

}
