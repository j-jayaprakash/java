package com.jp.ums;


import com.jp.ums.dto.User;


public class App 
{
    public static void main( String[] args )

    {
    	
    	User u1 = new User();
    	
    	u1.setName("jayaprakash");
    	u1.setEmail("jayaprakash@gmail.com");
    	
    	StringBuilder sb = new StringBuilder();
    	
    	
    	sb.append("insert into user_info");
    	sb.append("columns (`user_name`,`user_email`)");
    	sb.append("values (");
    	sb.append(u1.getName());
    	sb.append(",");
    	sb.append(u1.getEmail());
    	sb.append(")");
    }
}
